  "version": "0.2.0",
  "configurations": [
    {
            "name": "Debug Expo Web",
            "request": "launch",
            "type": "chrome",
            "url": "http://localhost:19006",
            "webRoot": "${workspaceFolder}",
            "sourceMaps": true,
            "runtimeExecutable": "${env:EXPO_WEB_BROWSER}",
            "runtimeArgs": [
                "--start-server"
            ],
            "preLaunchTask": "npm: web"
        }
  ]
}
